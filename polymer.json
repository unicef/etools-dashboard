{
  "entrypoint": "index.html",
  "shell": "src/app-shell.js",
  "fragments": [
    "src/views/hact/view-hact.js",
    "src/views/personalized/view-personalized.js",
    "src/views/partnerships/view-partnerships.js",
    "src/views/map/view-map.js",
    "src/views/trips/view-trips.js",
    "src/views/attachments/view-attachments.js"
  ],
  "sources": ["images/**/*", "dash/**/*"],
  "extraDependencies": [
    "node_modules/webcomponentsjs/*.js",
    "manifest.json"
  ],
  "lint": {
    "rules": ["polymer-3"]
  },
  "builds": [
    {
      "name": "esm-bundled",
      "browserCapabilities": [
        "es2015",
        "modules"
      ],
      "js": {
        "minify": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      },
      "bundle": true,
      "addServiceWorker": true
    },
    {
      "name": "es6-bundled",
      "preset": "es6-bundled",
      "addServiceWorker": true
    },
    {
      "name": "es5-bundled",
      "preset": "es5-bundled",
      "addServiceWorker": true
    }
  ],
  "moduleResolution": "node",
  "npm": true
}
