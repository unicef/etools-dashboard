{
  "entrypoint": "index.html",
  "shell": "dash/app-shell.html",
  "fragments": [
    "dash/app/views/hact/hact.html",
    "dash/app/views/personalized/personalized.html"
  ],
  "sources": ["images/**/*", "dash/**/*"],
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js",
    "!bower_components/webcomponentsjs/gulpfile.js",
    "manifest.json"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [
    {
      "preset": "es5-bundled"
    },
    {
      "preset": "es6-bundled"
    },
    {
      "preset": "es6-unbundled"
    }
  ]
}
