    "@webcomponents/webcomponentsjs": "^2.6.0",
    "cryptiles": ">=4.1.3",
    "dayjs": "^1.10.7",
    "lodash": ">=4.17.21",
    "lodash-es": "^4.17.21",
    "pwa-helpers": "^0.9.1",
    "ramda": "^0.27.1",
