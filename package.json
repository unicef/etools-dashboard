{
  "name": "dashboard",
  "version": "3.0.0",
  "author": "The eTools Team",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git@github.com:unicef/etools-dashboard"
  },
  "description": "Dashboard for UNICEF eTools",
  "scripts": {
    "build": "export NODE_OPTIONS=--max_old_space_size=8192 && npm run tsToJs && npm run build:static",
    "tsToJs": "tsc --skipLibCheck",
    "tsEslint": "eslint src_ts/**",
    "start": "gulp prpl-server serve",
    "build:prpl-server": "polymer build --auto-base-path && gulp prpl-server",
    "build:static": "polymer build",
    "serve:prpl-server": "prpl-server --root server/build",
    "serve:static": "polymer serve --port 5000 build/es5-bundled",
    "test": "npm run test:unit && npm run test:integration",
    "test:integration": "tsc && mocha test/integration --timeout=10000",
    "test:unit": "tsc && polymer test --module-resolution=node --npm",
    "test:sauce": "tsc && polymer test --module-resolution=node --npm -s 'windows 10/microsoftedge@17' -s 'macos 10.13/safari@11'",
    "test:regenerate_screenshots": "tsc && mocha test/integration/screenshots-baseline/regenerate.js --timeout=15000"
  },
  "dependencies": {
    "@polymer/app-layout": "^3.0.0-pre.18",
    "@polymer/app-route": "^3.0.0-pre.18",
    "@polymer/decorators": "^3.0.0",
    "@polymer/iron-flex-layout": "^3.0.0-pre.18",
    "@polymer/iron-icons": "^3.0.0-pre.18",
    "@polymer/iron-iconset-svg": "^3.0.0-pre.18",
    "@polymer/iron-pages": "^3.0.0-pre.18",
    "@polymer/paper-button": "^3.0.0-pre.18",
    "@polymer/paper-dropdown-menu": "^3.1.0",
    "@polymer/paper-icon-button": "^3.0.0-pre.18",
    "@polymer/paper-item": "^3.0.0-pre.18",
    "@polymer/paper-listbox": "^3.0.0-pre.18",
    "@polymer/paper-menu-button": "^3.0.0-pre.18",
    "@polymer/paper-styles": "^3.0.0-pre.18",
    "@polymer/paper-tabs": "^3.0.0-pre.18",
    "@polymer/paper-toast": "^3.0.0-pre.18",
    "@polymer/polymer": "^3.1.0",
    "@types/lodash-es": "^4.17.3",
    "@unicef-polymer/etools-app-selector": "^3.1.5",
    "@unicef-polymer/etools-behaviors": "^3.0.1",
    "@unicef-polymer/etools-loading": "^4.0.1",
    "@unicef-polymer/etools-profile-dropdown": "^4.0.1-rc.1",
    "@webcomponents/webcomponentsjs": "^2.0.0",
    "child_process": "^1.0.2",
    "cryptiles": ">=4.1.2",
    "dayjs": "^1.10.4",
    "del": "^5.0.0",
    "etools-piwik-analytics": "git+https://github.com/unicef-polymer/etools-piwik-analytics.git#upgrade-to-polymer3",
    "express": "^4.16.3",
    "extend": ">=3.0.2",
    "gulp": "^4.0.2",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "js-yaml": ">=3.13.1",
    "lodash-es": ">=4.17.14",
    "stringstream": ">=0.0.6"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "@typescript-eslint/typescript-estree": "^1.12.0",
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.7.1",
    "eslint-plugin-html": "^4.0.3",
    "polymer-cli": "^1.9.11",
    "prpl-server": "^1.0.0",
    "typescript": "^3.5.3",
    "wct-browser-legacy": "^1.0.1"
  },
  "resolutions": {
    "inherits": "2.0.3",
    "samsam": "1.1.3",
    "supports-color": "3.1.2",
    "type-detect": "1.0.0"
  }
}
