{
  "name": "dashboard",
  "version": "3.0.0",
  "author": "The eTools Team",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git@github.com:unicef/etools-dashboard"
  },
  "description": "Dashboard for UNICEF eTools",
  "scripts": {
    "build": "export NODE_OPTIONS=--max_old_space_size=8192 && npm run tsToJs && npm run build:static",
    "tsToJs": "tsc --skipLibCheck",
    "tsEslint": "eslint src_ts/**",
    "start": "tsc --watch --preserveWatchOutput & polymer serve -H 0.0.0.0 -p 8080",
    "build:prpl-server": "polymer build --auto-base-path && gulp prpl-server",
    "build:static": "polymer build",
    "serve:prpl-server": "prpl-server --root server/build",
    "serve:static": "polymer serve --port 5000 build/es5-bundled",
    "test": "npm run test:unit && npm run test:integration",
    "test:integration": "tsc && mocha test/integration --timeout=10000",
    "test:unit": "tsc && polymer test --module-resolution=node --npm",
    "test:sauce": "tsc && polymer test --module-resolution=node --npm -s 'windows 10/microsoftedge@17' -s 'macos 10.13/safari@11'",
    "test:regenerate_screenshots": "tsc && mocha test/integration/screenshots-baseline/regenerate.js --timeout=15000"
  },
  "dependencies": {
    "@polymer/app-layout": "^3.0.1",
    "@polymer/app-route": "^3.0.2",
    "@polymer/decorators": "^3.0.0",
    "@polymer/iron-flex-layout": "^3.0.1",
    "@polymer/iron-icons": "^3.0.1",
    "@polymer/iron-iconset-svg": "^3.0.1",
    "@polymer/iron-pages": "^3.0.1",
    "@polymer/paper-button": "^3.0.1",
    "@polymer/paper-dropdown-menu": "^3.1.0",
    "@polymer/paper-icon-button": "^3.0.2",
    "@polymer/paper-item": "^3.0.1",
    "@polymer/paper-listbox": "^3.0.1",
    "@polymer/paper-menu-button": "^3.0.1",
    "@polymer/paper-styles": "^3.0.1",
    "@polymer/paper-tabs": "^3.1.0",
    "@polymer/paper-toast": "^3.0.1",
    "@polymer/paper-toggle-button": "^3.0.1",
    "@polymer/paper-tooltip": "^3.0.1",
    "@polymer/polymer": "^3.4.1",
    "@types/lodash-es": "^4.17.4",
    "@unicef-polymer/etools-ajax": "^4.2.0",
    "@unicef-polymer/etools-app-selector": "^3.1.5",
    "@unicef-polymer/etools-behaviors": "^3.0.4",
    "@unicef-polymer/etools-date-time": "^1.0.5",
    "@unicef-polymer/etools-info-tooltip": "^3.0.1",
    "@unicef-polymer/etools-loading": "^4.0.4",
    "@unicef-polymer/etools-profile-dropdown": "^4.0.5",
    "@webcomponents/webcomponentsjs": "^2.5.0",
    "cryptiles": ">=4.1.2",
    "dayjs": "^1.10.4",
    "etools-piwik-analytics": "git+https://github.com/unicef-polymer/etools-piwik-analytics.git#upgrade-to-polymer3",
    "lodash": ">=4.17.19",
    "lodash-es": "^4.17.20",
    "pwa-helpers": "^0.9.1",
    "ramda": "^0.27.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "stringstream": ">=0.0.6"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "@typescript-eslint/typescript-estree": "^3.10.1",
    "eslint": "^7.20.0",
    "eslint-config-google": "^0.13.0",
    "eslint-plugin-html": "^6.1.1",
    "extend": ">=3.0.2",
    "gulp": "^4.0.2",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "js-yaml": "^4.0.0",
    "polymer-cli": "^1.9.11",
    "typescript": "^3.9.7"
  }
}
