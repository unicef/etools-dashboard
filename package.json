{
  "name": "dashboard",
  "version": "3.0.0",
  "author": "The eTools Team",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git@github.com:unicef/etools-dashboard"
  },
  "description": "Dashboard for UNICEF eTools",
  "scripts": {
    "tsToJs": "rm -rf ./src && tsc --skipLibCheck",
    "tsEslint": "node_modules/eslint/bin/eslint.js 'src_ts/**/*.ts' --fix",
    "start": "rm -rf src && rollup -c rollup-dev.config.js -w",
    "start:tsc": "tsc || true && concurrently -k -r \"tsc --watch --preserveWatchOutput\" \"wds --hmr\"",
    "build": "export NODE_OPTIONS=--max_old_space_size=4096 && rollup -c rollup-prod.config.js",
    "lint:eslint": "eslint --ext .ts . --ignore-path .gitignore",
    "format:eslint": "eslint --ext .ts . --fix --ignore-path .gitignore",
    "lint:prettier": "prettier \"**/*.ts\" --check --ignore-path .gitignore",
    "format:prettier": "prettier \"**/*.ts\" --write --ignore-path .gitignore",
    "lint": "npm run lint:eslint",
    "format": "npm run format:eslint",
    "generate:icons": "npm --prefix node_modules/@unicef-polymer/etools-unicef run generate-icons -- --output=$(pwd)/assets/icons/ --icons=$(pwd)/assets/icons/icons.js",
    "rd:init": "relative-deps init",
    "rd:add": "relative-deps add",
    "rd:reload": "relative-deps",
    "rd:watch": "chokidar \"../../etools-polymer/**/*.js\" \"../../etools-polymer/**/**/*.js\" -c 'npm run rd:reload' --silent"
  },
  "dependencies": {
    "@types/lodash-es": "^4.17.5",
    "@shoelace-style/shoelace": "2.10.0",
    "@unicef-polymer/etools-modules-common": "^5.0.0-rc.54",
    "@unicef-polymer/etools-piwik-analytics": "4.0.1-rc.3",
    "@unicef-polymer/etools-types": "^1.11.17",
    "@unicef-polymer/etools-unicef": "^1.0.0-rc.101",
    "@unicef-polymer/etools-utils": "^2.0.0-rc.14",
    "@webcomponents/webcomponentsjs": "^2.6.0",
    "lit": "^2.7.5",
    "lit-translate": "^2.0.1",
    "cryptiles": ">=4.1.3",
    "dayjs": "^1.10.7",
    "lodash-es": "^4.17.21",
    "pwa-helpers": "^0.9.1",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1",
    "stringstream": ">=0.0.6",
    "dexie": "^3.2.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-dynamic-import-vars": "^2.1.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-terser": "0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "@typescript-eslint/typescript-estree": "^5.6.0",
    "ajv": ">=6.12.6",
    "async": ">=2.6.4",
    "decode-uri-component": ">=0.2.2",
    "engine.io": ">=3.6.1",
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-lit": "^1.6.1",
    "eslint-plugin-lit-a11y": "^1.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "extend": ">=3.0.2",
    "follow-redirects": ">=1.14.9",
    "js-yaml": "^4.0.0",
    "json5": ">=2.2.3",
    "minimatch": ">=3.1.2",
    "mout": ">=1.2.4",
    "node-fetch": ">=2.6.7",
    "path-parse": ">=1.0.7",
    "prettier": "~2.5.1",
    "qs": ">=6.5.3",
    "rollup": "^2.79.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-esbuild": "^6.1.0",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-license": "^3.2.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-serve": "^2.0.2",
    "rollup-plugin-workbox": "^8.0.1",
    "shelljs": ">=0.8.5",
    "smob": "1.4.1",
    "socket.io": ">=2.5.0",
    "socket.io-parser": ">=3.3.3",
    "thenify": ">=3.3.1",
    "typescript": "^3.9.7",
    "ua-parser-js": ">=1.0.33",
    "urijs": ">=1.19.11",
    "wd": ">=1.14.0"
  }
}
