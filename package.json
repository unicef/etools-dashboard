{
  "name": "dash",
  "version": "0.0.0",
  "description": "eTools Dashboard Portal",
  "repository": {
    "type": "git",
    "url": "git@github.com:unicef/etools-dashboard"
  },
  "license": "http://polymer.github.io/LICENSE.txt",
  "bugs": {
    "url": "https://github.com/unicef/etools-partnership-management/blob/master/LICENSE.md"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "ie >= 11"
            ]
          }
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "engines": {
    "node": ">=4.0"
  },
  "scripts": {
    "dev": "polymer serve & npm run watch",
    "test": "polymer test",
    "watch": "browser-sync start --proxy localhost:8082 --files '**/*.html, **/*.js, images/*'",
    "lint": "npm run lint:javascript && exit 0",
    "lint:javascript": "eslint . --ext js,html && exit 0"
  },
  "private": true,
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "browser-sync": "^2.18.7",
    "css-slam": "^1.1.0",
    "del": "^2.2.1",
    "eslint": "^3.19.0",
    "eslint-config-google": "^0.7.0",
    "eslint-plugin-html": "^2.0.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-babel": "^7.0.0",
    "gulp-crisper": "^1.1.0",
    "gulp-csslint": "^1.0.0",
    "gulp-html-extract": "^0.2.0",
    "gulp-htmlhint": "^0.3.1",
    "gulp-htmlmin": "^2.0.0",
    "gulp-if": "^2.0.1",
    "gulp-imagemin": "^3.0.1",
    "gulp-jscs": "^4.0.0",
    "gulp-jscs-stylish": "^1.4.0",
    "gulp-jshint": "^2.0.1",
    "gulp-load-plugins": "^1.5.0",
    "gulp-replace": "^0.5.4",
    "gulp-uglify": "^2.0.0",
    "gulp-useref": "^3.1.2",
    "gulp-util": "^3.0.7",
    "htmlhint-stylish": "^1.0.3",
    "jshint": "^2.9.3",
    "jshint-stylish": "^2.2.1",
    "lazypipe": "^1.0.1",
    "merge-stream": "^1.0.0",
    "plylog": "^0.4.0",
    "polymer-build": "^0.4.0",
    "prettier-eslint": "^8.0.0",
    "uglify-js-harmony": "^2.7.7",
    "yargs": "^5.0.0"
  },
  "dependencies": {
    "express": "^4.14.0",
    "redux": "^3.6.0",
    "uglify-js-harmony": "^2.7.7"
  }
}
